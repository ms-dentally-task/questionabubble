<div>
  <%= form_with(model: vote, class: "contents") do |form| %>
    <%= form.hidden_field :voteable_id %>
    <%= form.hidden_field :voteable_type %>
    <div class="rounded-md py-2 px-3 bg-slate-300 inline-block font-medium cursor-pointer flex">
      <%= image_tag 'bubble.svg', class: 'pr-2', alt: '' %>
      <%= form.submit 'Bubble Up!' %>
    </div>
  <% end %>
</div>
